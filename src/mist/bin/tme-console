#!/bin/sh
javacmd="/usr/java/latest/bin/java"
if [ ! -x "$javacmd" ]; then
    echo "Sun Java Runtime Environment: $javacmd not found"
    exit 1
fi
$javacmd -cp '/opt/trend/tme/lib/*' -Djava.security.auth.login.config=/opt/trend/tme/conf/mist/ldaploginmodule.conf com.trendmicro.mist.cmd.TmeConsole $*
exit $?
